    <properties>
        <path.resources.generated>generated-resources</path.resources.generated>
        <path.sources.generated>generated-sources</path.sources.generated>
        <root.groupId.dot>com.hyper9</root.groupId.dot>
        <root.groupId.slash>com/hyper9</root.groupId.slash>
        <root.artifactId>simdk</root.artifactId>
        <root.packagename.dot>${root.groupId.dot}.${root.artifactId}</root.packagename.dot>
        <root.packagename.slash>${root.groupId.slash}/${root.artifactId}</root.packagename.slash>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <vmware.target.version>4.0</vmware.target.version>
        <hibernate.dialect>org.hibernate.dialect.MySQLDialect</hibernate.dialect>
        <hibernate.connection.driver_class>com.mysql.jdbc.Driver</hibernate.connection.driver_class>
        <hibernate.connection.url>jdbc:mysql://localhost/db_simdk</hibernate.connection.url>
        <hibernate.connection.username>root</hibernate.connection.username>
        <hibernate.connection.password>password</hibernate.connection.password>
        <hibernate.hbm2ddl.auto />
        <hibernate.show_sql>false</hibernate.show_sql>
    </properties>
    <modules>
        <module>codegen</module>
        <module>stubs</module>
        <module>db</module>
        <module>viexport</module>
        <module>ws</module>
        <module>dist</module>
    </modules>
