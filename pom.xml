        </repository>
        <snapshotRepository>
            <id>maven.lostcreations.com-snapshots</id>
            <name>maven.lostcreations.com-snapshots</name>
            <url>scp://maven.lostcreations.com/home/akutz/maven.lostcreations.com/snapshots/</url>
        </snapshotRepository>
        <site>
            <id>sourceforge.net</id>
            <name>sourceforge.net</name>
            <url>scp://shell.sourceforge.net/home/groups/s/si/simdk/htdocs</url>
        </site>
